datasource db {
  provider = "mysql"
  url      = env("DATABASE_URI")
  relationMode = "prisma"
}
generator client {
  provider = "prisma-client-js"
  previewFeatures = ["prismaSchemaFolder"]
}


model Client{
    id String @id @default(uuid())
    companyName String
    contactPerson String
    GST String
    mobileNum String
    address String
    status String
    createdAt DateTime @default(now())
    updataedAt DateTime @updatedAt
}